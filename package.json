{
  "name": "gemini-agent-workflow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:vite": "nodemon --exec vite",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      ".env"
    ],
    "ext": "ts,tsx,js,jsx,json",
    "ignore": [
      "node_modules",
      "dist",
      "build",
      "*.test.ts",
      "*.test.js"
    ],
    "delay": "500",
    "exec": "vite"
  },
  "dependencies": {
    "@google/genai": "^1.33.0",
    "@google/generative-ai": "^0.21.0",
    "@huggingface/inference": "^4.13.5",
    "docx": "^8.5.0",
    "lucide-react": "^0.263.1",
    "mammoth": "^1.8.0",
    "openai": "^6.10.0",
    "pdfjs-dist": "^3.11.174",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "save": "^2.9.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/pdfjs-dist": "^2.10.377",
    "@vitejs/plugin-react": "^5.0.0",
    "nodemon": "^3.1.11",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
